       <Grid item xs={12} sm={6}>\n            <TextField\n              fullWidth\n              label=\"Χρώμα Γραφήματος\"\n              name=\"color\"\n              type=\"color\"\n              value={settings.color}\n              onChange={handleChange}\n            />\n          </Grid>\n          <Grid item xs={12}>\n            <Button variant=\"contained\" color=\"primary\">\n              Εφαρμογή Ρυθμίσεων\n            </Button>\n          </Grid>\n        </Grid>\n      </Paper>\n    </Box>\n  );\n}\n\nexport default ChartSettings; "],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,QAAQ,KAAQ,OAAO,CACvC,OACEC,GAAG,CACHC,KAAK,CACLC,UAAU,CACVC,WAAW,CACXC,UAAU,CACVC,MAAM,CACNC,QAAQ,CACRC,SAAS,CACTC,MAAM,CACNC,IAAI,KACC,eAAe,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAEvB,QAAS,CAAAC,aAAaA,CAAA,CAAG,CACvB,KAAM,CAACC,QAAQ,CAAEC,WAAW,CAAC,CAAGjB,QAAQ,CAAC,CACvCkB,SAAS,CAAE,KAAK,CAChBC,KAAK,CAAE,EAAE,CACTC,UAAU,CAAE,EAAE,CACdC,UAAU,CAAE,EAAE,CACdC,KAAK,CAAE,SACT,CAAC,CAAC,CAEF,KAAM,CAAAC,YAAY,CAAIC,CAAC,EAAK,CAC1B,KAAM,CAAEC,IAAI,CAAEC,KAAM,CAAC,CAAGF,CAAC,CAACG,MAAM,CAChCV,WAAW,CAAC,CAAE,GAAGD,QAAQ,CAAE,CAACS,IAAI,EAAGC,KAAM,CAAC,CAAC,CAC7C,CAAC,CAED,mBACEZ,KAAA,CAACb,GAAG,EAAA2B,QAAA,eACFhB,IAAA,CAACT,UAAU,EAAC0B,OAAO,CAAC,IAAI,CAACC,YAAY,MAAAF,QAAA,CAAC,qHAEtC,CAAY,CAAC,cACbhB,IAAA,CAACV,KAAK,EAAC6B,EAAE,CAAE,CAAEC,CAAC,CAAE,CAAE,CAAE,CAAAJ,QAAA,cAClBd,KAAA,CAACJ,IAAI,EAACuB,SAAS,MAACC,OAAO,CAAE,CAAE,CAAAN,QAAA,eACzBhB,IAAA,CAACF,IAAI,EAACyB,IAAI,MAACC,EAAE,CAAE,EAAG,CAACC,EAAE,CAAE,CAAE,CAAAT,QAAA,cACvBd,KAAA,CAACV,WAAW,EAACkC,SAAS,MAAAV,QAAA,eACpBhB,IAAA,CAACP,UAAU,EAAAuB,QAAA,CAAC,6FAAgB,CAAY,CAAC,cACzCd,KAAA,CAACR,MAAM,EACLmB,IAAI,CAAC,WAAW,CAChBC,KAAK,CAAEV,QAAQ,CAACE,SAAU,CAC1BqB,KAAK,CAAC,6FAAkB,CACxBC,QAAQ,CAAEjB,YAAa,CAAAK,QAAA,eAEvBhB,IAAA,CAACL,QAAQ,EAACmB,KAAK,CAAC,KAAK,CAAAE,QAAA,CAAC,oEAAW,CAAU,CAAC,cAC5ChB,IAAA,CAACL,QAAQ,EAACmB,KAAK,CAAC,KAAK,CAAAE,QAAA,CAAC,uFAAe,CAAU,CAAC,cAChDhB,IAAA,CAACL,QAAQ,EAACmB,KAAK,CAAC,MAAM,CAAAE,QAAA,CAAC,yGAAkB,